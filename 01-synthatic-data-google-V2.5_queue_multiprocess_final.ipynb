    "\n",
    "# Load adult.csv from the downloaded dataset\n",
    "# The dataset typically downloads to a folder containing adult.csv\n",
    "data_path = os.path.join(path, \"adult.csv\")\n",
    "data = pd.read_csv(data_path)\n",
    "# Sample a subset (e.g., 100 rows) as seed data\n",
    "seed_data = data.sample(n=100, random_state=42)"
