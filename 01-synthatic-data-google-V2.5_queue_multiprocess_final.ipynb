    "    n_batches=1000,\n",
    "    n_examples=10,\n",
    "    random_state=42,                # reproducible sampling of examples\n",
    "    outfile_csv=\"synthetic_adult_census_mp.csv\",\n",
    "    outfile_log=\"synthetic_mp_log.jsonl\",\n",
